	url = git@heroku.com:pledgebench.git
[branch "master"]
	remote = origin
	merge = refs/heads/master
[remote "all"]
	url = git@github.com:AaronRandall/pledgebench.git
	fetch = +refs/heads/*:refs/remotes/all/*
	pushurl = git@heroku.com:pledgebench.git
	pushurl = git@github.com:AaronRandall/pledgebench.git

