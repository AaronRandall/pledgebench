<div class="centered">
  <h2>All listings</h2>
  <h3><small>Here are all the current listings from Pledgebench volunteers.</small></h3>

  <table>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Volunteer email</th>
    </tr>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.title %></td>
        <td><%= listing.description %></td>
        <td><%= listing.user.email %></td>
      </tr>
    <% end %>
  </table>
  
</div>
